<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.jdt.core.classpathContainerInitializer">
      <classpathContainerInitializer
            class="edu.cmu.cs.planno.PlaidClasspathContainerInitializer"
            id="edu.cmu.cs.planno.PLAID_ANNOTATION_CONTAINER">
      </classpathContainerInitializer>
   </extension>
   <extension
         point="org.eclipse.jdt.ui.classpathContainerPage">
      <classpathContainerPage
            class="edu.cmu.cs.planno.PlaidAnnotationContainerPage"
            id="edu.cmu.cs.planno.PLAID_ANNOTATION_CONTAINER"
            name="Plaid Annotations">
      </classpathContainerPage>
   </extension>
   <extension
         point="org.eclipse.jdt.ui.classpathFixProcessors">
      <classpathFixProcessor
            class="edu.cmu.cs.planno.PlaidClasspathFix"
            id="edu.cmu.cs.planno.plaidClasspathFix"
            name="Plaid Annotations Classpath Fix">
      </classpathFixProcessor>
   </extension>
   <extension
         point="org.eclipse.jdt.ui.quickFixProcessors">
      <quickFixProcessor
            class="edu.cmu.cs.planno.PlaidQuickFixProcessor"
            id="edu.cmu.cs.planno.pladQuickFixProcessor"
            name="Plaid Annotations QuickFix">
      </quickFixProcessor>
   </extension>

</plugin>
